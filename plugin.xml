<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               id="edu.ualberta.med.scannerconfig.menu"
               label="Scanner">
            <command
                  commandId="edu.ualberta.med.scannerconfig.menu.commands.source"
                  label="Select Source"
                  style="push">
            </command>
            <command
                  commandId="edu.ualberta.med.scannerconfig.menu.commands.settings"
                  label="Configuration"
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="edu.ualberta.med.scannerconfig.ScannerSettingsHandler"
            id="edu.ualberta.med.scannerconfig.menu.commands.settings"
            name="Settings">
      </command>
      <command
            defaultHandler="edu.ualberta.med.scannerconfig.SelectSourceHandler"
            id="edu.ualberta.med.scannerconfig.menu.commands.source"
            name="Select Source">
      </command>
   </extension>

</plugin>
